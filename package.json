{
  "name": "autocare-pro-dashboard",
  "version": "2.1.0",
  "description": "Dashboard professionnel pour la gestion de flotte de vÃ©hicules - nettoyage et entretien",
  "private": true,
  "homepage": "https://altair3s.github.io/pronet",
  "keywords": [
    "dashboard",
    "fleet-management",
    "vehicle-maintenance",
    "car-washing",
    "business-management",
    "react",
    "tailwindcss"
  ],
  "author": {
    "name": "AutoCare Pro",
    "email": "contact@autocare-pro.fr"
  },
  "dependencies": {
    "firebase": "^11.9.0",
    "lucide-react": "^0.263.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "ajv": "^7.2.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy":"npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,css,md,json}",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "serve": "npx serve -s build -l 3000",
    "clean": "rm -rf build node_modules package-lock.json && npm install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "react/prop-types": "warn",
      "react/no-unescaped-entities": "off"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-username/autocare-pro-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/votre-username/autocare-pro-dashboard/issues"
  },
  "license": "MIT"
}
